{
  "name": "my-fullstack-app",
  "version": "1.0.0",
  "scripts": {
    "build:client": "cd client && npm run build",
    "install:client": "cd client && npm install",
    "start:server": "cd server && npm start",
    "dev": "concurrently \"cd server && npm run dev\" \"cd client && npm start\""
  },
  "devDependencies": {
    "concurrently": "^8.0.0"
  },
  "engines": {
    "node": ">=18.x"
  },
  "dependencies": {
    "multer": "^2.0.2"
  }
}
