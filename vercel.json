{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "test-app-omega-teal.vercel.app"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}